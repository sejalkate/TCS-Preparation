package tcs_practice;
public class FactorialUsingRecursion
{
	public static int fact(int num)
	{
		if(num==0 || num==1)
		{
			return 1;
		}
		else
		{
			return num*fact(num-1);
		}
	}
	public static void main(String[] args) 
	{
		int num=5;
		int res=fact(num);
		System.out.println("Factorial of "+num+"="+res);
	}
}
